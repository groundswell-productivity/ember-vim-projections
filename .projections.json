    ]
  },

  "app/components/*.js": {
    "command": "component",
    "template": [
      "import Component from '@ember/component';",
      "export default Component.extend({",
      "",
      "});",
    ]
  },

  "app/controllers/*.js": {
    "command": "fcontroller",
    "template": [
      "import Controller from '@ember/controller';",
      "export default Controller.extend({",
      "",
      "});",
    ]
  },

  "app/helpers/*.js": {
    "command": "fhelper",
    "template": [
