{
  "app/serializers/*.js": {
    "command": "fserializer",
    "template": [
      "import JSONAPISerializer from 'ember-data/serializers/json-api;",
      "export default JSONAPISerializer.extend({",
      "",
      "});",
    ]
  },

  "app/adapters/*.js": {
    "command": "fadapter",
    "template": [
      "import JSONAPIAdapter from 'ember-data/adapters/json-api';",
      "export default JSONAPIAdapter.extend({",
      "",
      "});"
    ]
  },

  "tests/acceptance/*.js": {
