      "});"
    ]
  },

  "mirage/scenarios/*.js": {
    "command": "fscenario"
  },

  "app/mixins/*.js": {
    "command": "mixin",
    "template": [
      "import Mixin from '@ember/object/mixin';",
      "export default Mixin.create({",
      "",
      "});",
    ]
  },
