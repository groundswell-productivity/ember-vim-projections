  "app/models/*.js": {
    "command": "fmodel",
    "template": [
      "import attr from 'ember-data/attr';",
      "import Model from 'ember-data/model';",
      "export default Model.extend({",
      "",
      "});",
    ]
  },

  "app/router.js": {
