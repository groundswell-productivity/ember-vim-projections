  },

  "app/routes/*.js": {
    "command": "route",
    "template": [
      "import Route from '@ember/routing/route';",
      "export default Route.extend({",
      "",
      "});",
    ]

  },

  "app/services/*.js": {
    "command": "service",
    "template": [
      "import Service from '@ember/service';",
      "export default Service.extend({",
      "",
      "});",
    ]
  },

  "app/styles/*.scss": {
    "command": "style",
    "template": [
      "/* {}.scss */",
      "",
    ]
  },

  "app/templates/*.hbs": {
    "command": "template"
  },

  "app/templates/*.em": {
    "command": "template"
  },

  "app/utils/*.js": {
    "command": "util"
  },

  "mirage/config.js": {
